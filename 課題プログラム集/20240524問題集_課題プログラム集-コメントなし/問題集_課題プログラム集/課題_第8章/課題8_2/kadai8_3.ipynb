#Anaconda

import turtle
def draw_spiral(size):
    if size > 30:
        return
    kameo.forward(size)
    kameo.right(15)
    return draw_spiral(size*1.02) 

screen = turtle.Screen() 
screen.setup(600, 600)
screen.title('My Turtle Graphics')
kameo = turtle.Turtle() 
draw_spiral(5) 
screen.mainloop()
