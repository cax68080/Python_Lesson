#課題3.9　 

from fractions import Fraction
N = 200
M = 200
list_t1 = [Fraction((-1) ** n, (2 * n + 1) * 5 ** (2 * n + 1)) for n in range(N)]
list_t2 = [Fraction((-1) ** n, (2 * n + 1) * 239 ** (2 * n + 1)) for n in range(M)]
sum1 = sum(list_t1)
sum2 = sum(list_t2)
print('πの近似値：', 16 * float(sum1) - 4 * float(sum2))

