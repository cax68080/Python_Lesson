#課題7.14＊ 

class PetOwner:
  def __init__(self, name):
    self.name = name
  def message(self, cat): 
    print(self.name + ' feeds his cat ' + cat.name + ', ' \
    + cat.breed +'. ' + cat.meow())
class Cat:
  def __init__(self, name, breed):
    self.name = name 
    self.breed = breed
  def meow(self): 
    return 'Meow!'
    
# メインのプログラム
cat = Cat('Luna', 'American Shorthair') 
petowner = PetOwner('John') 
petowner.message(cat) 
