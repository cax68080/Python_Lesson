#課題7.5　 

class Fibonacci:
  def __init__(self, x = 0, y = 1): 
    self.x = x 
    self.y = y
  def fibo(self, n):
    self.x, self.y = 0, 1
    for i in range(n):
      self.x, self.y = self.y, self.x + self.y
    return self.y 


fib_obj = Fibonacci() 
print([fib_obj.fibo(i) for i in range(20)])

