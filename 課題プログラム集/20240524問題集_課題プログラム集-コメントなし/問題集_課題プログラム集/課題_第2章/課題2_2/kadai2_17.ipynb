def fizzbuzz(num):
  if num % 3 == 0 and num % 5 == 0:       
    return 'Fizz Buzz'
  elif num % 3 == 0: 
    return 'Fizz'
  elif num % 5 == 0: 
    return 'Buzz'
  else: 
   return num

n = int(input('Enter an integer n = '))
listx = list(range(1, n+1))
print(list(map(fizzbuzz, listx))) 
