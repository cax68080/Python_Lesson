#課題2.21  

def palindrome(word): 
  i = 0 
  j = len(word) - 1
  while i < j: 
    if word[i] != word[j]: 
      return False
    i += 1
    j -= 1
  return True
words = ['akasaka', 'asakusa', 'ABBA', '1234554321', '12354321']
list_pal = []
for i in filter(palindrome, words):
  list_pal.append(i)
print(list_pal) 
