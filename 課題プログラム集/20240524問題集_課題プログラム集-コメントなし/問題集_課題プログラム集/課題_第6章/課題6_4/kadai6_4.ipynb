import numpy as np
import matplotlib.pyplot as plt
r, K, N0 = 0.01, 1000, 10 
x = np.arange(0, 1001, 1) 
y = K * N0 * np.exp(r*x) / (N0 * np.exp(r*x) + K - N0)
plt.plot(x, y)
plt.title('increasing numbers of a living organism')
plt.xlabel('time')
plt.ylabel('numbers')
plt.grid()
plt.show()
