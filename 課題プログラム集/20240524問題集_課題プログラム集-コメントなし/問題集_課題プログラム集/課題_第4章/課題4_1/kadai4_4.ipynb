#課題4.4　 

import sympy
num = 31
list_num =[i for i in str(num)]
print(list_num)
print(num)
for i in range(1, 10):
  if sympy.isprime(num):
    print(num, True)
  else:
    print(num, False)
  if i <= 8:
    list_num.insert(0, '3')
    print(list_num, len(list_num))
    num = int(''.join(list_num))
    print(num)

