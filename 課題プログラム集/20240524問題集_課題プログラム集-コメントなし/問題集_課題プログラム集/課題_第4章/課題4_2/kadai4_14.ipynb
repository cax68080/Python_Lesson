#課題4.14　 

from sympy import factorint
def factorization(n):
  dictn = factorint(n) 
  sumn = 0
  for k, v in dictn.items():
    sumn += k * v 
  return n, dictn, sumn  

x = int(input('Enter a digit: '))
x, dictx, sumx = factorization(x)
print(x, dictx, sumx) 
y = x + 1 
y, dicty, sumy = factorization(y)
print(y, dicty, sumy)
if sumx == sumy: 
  print(True)
else:
  print(False)
