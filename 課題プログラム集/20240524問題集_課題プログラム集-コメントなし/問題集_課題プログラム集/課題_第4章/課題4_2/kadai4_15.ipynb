#課題4.15

def divisors(n):
  list_divisors = []
  for i in range(1, n+1): 
    if n % i == 0:
      list_divisors.append(i)
  return list_divisors, len(list_divisors)

n = int(input('Enter an integer n = '))
listx = []
for i in range(1, n+1):
  listx.append(i**2)
print(listx)
for j in listx:
  list_divisors, size = divisors(j)
  if size % 2 != 0: 
    print('平方数', j, ' 約数のリスト', list_divisors, ' 個数', size, ' ', True)
