#課題1.36＊　

set_F, set_B, set_FB = set(), set(), set()        
for num in range(1, 21): 
  if num % 3 == 0 and num % 5 == 0:      
    set_FB.add(num)
  elif num % 3 == 0: 
    set_F.add(num)
  elif num % 5 == 0: 
    set_B.add(num)
print('set of Fizzes: ', set_F, len(set_F)) 
print('set of Buzzes: ', set_B, len(set_B)) 
print('set of FizzBuzzes: ', set_FB, len(set_FB)) 
