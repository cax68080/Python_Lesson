#課題1.32　

year = {'1月':31, '2月':28, '3月':31, '4月':30, '5月':31, '6月':30, 
'7月':31, '8月':31, '9月':30, '10月':31, '11月':30, '12月':31}
large = [] 
small = []
for m, d in year.items(): 
if d == 31:
large.append(m) 
else:
small.append(m)
print('大の月', large)
print('小の月', small)


